<body class="coming-page">

    <!--COMMING WRAP BEGIN-->
    <div class="coming-wrap">
        <div class="container">
            <div class="row">
                <div class="col-md-12" style="margin-top:15px; margin-bottom:15px">
                    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#EmBreve" data-backdrop="static">
                        <b>Fechar</b>
                    </button>
                </div>
            </div>
        </div>
        <div class="coming-title">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <span class="small-text">
                            <span class="logo"><img src="~/images/soccer/petrolandia.png" width="60" height="60" alt="team-logo"></span> 
                            A definir - A definir
                            <span class="logo"><img src="~/images/soccer/amazonas2.png" width="60" height="60" alt="team-logo"></span>
                        </span>
                        <span class="big-text">final</span>
                        <div class="counter">
                            <ul>
                                <li>
                                    <div class="digit">@ViewBag.Dia</div>
                                    <div class="descr">dias</div>
                                </li>
                                <li>
                                    <div class="digit">@ViewBag.Hora</div>
                                    <div class="descr">horas</div>
                                </li>
                                <li>
                                    <div class="digit">@ViewBag.Minuto</div>
                                    <div class="descr">minutos</div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
